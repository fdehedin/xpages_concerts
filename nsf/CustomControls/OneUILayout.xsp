						legend="Search options and keywords" optionsParam="field"
						queryParam="searchstring" pageName="/view_schadenmeldungen.xsp">
					</xe:appSearchBar>
				</xe:this.searchBar>

				<xe:this.titleBarTabs>

					<xe:basicLeafNode label="Upcoming concerts" title="Upcoming concerts"
						selected="true">
					</xe:basicLeafNode>
				</xe:this.titleBarTabs>


			</xe:applicationConfiguration>
		</xe:this.configuration>
		<xp:callback id="OneUIMainAreaCallback"></xp:callback>
		<xe:dialog id="dialog1"></xe:dialog>
	</xe:applicationLayout>
</xp:view>
